  },
  "homepage": "https://github.com/fridge-cms/fridge.js#readme",
  "dependencies": {
    "cross-spawn": "^5.0.1",
    "fridge": "^2.0.4-beta",
    "hoist-non-react-statics": "^1.2.0",
    "lru-cache": "^4.0.2",
    "next": "^6.0.2",
    "next-route": "^2.0.0",
    "prop-types": "^15.6.0",
    "react-tree-walker": "^4.0.2",
    "serve": "^6.4.4"
  },
