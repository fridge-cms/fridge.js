{
  "name": "fridge-next",
  "version": "3.0.0",
  "description": "Build Fridge apps using next.js",
  "main": "./dist/index.js",
  "files": [
    "dist",
    "connectFridge.js",
    "withFridge.js"
  ],
  "bin": {
    "fridge": "./dist/bin/fridge.js"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run build",
